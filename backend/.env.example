# Backend Environment Variables
# Copy to .env for local development outside Docker

# Database connection string
# - Docker: postgresql://postgres:postgres@db:5432/notes_db (set in docker-compose.yml)
# - Non-Docker: postgresql://postgres:postgres@localhost:5432/notes_db
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/notes_db

# Allowed CORS origins (comma-separated)
CORS_ORIGINS=http://localhost:3000
